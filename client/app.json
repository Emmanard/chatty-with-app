{
  "expo": {
    "name": "ConvoWave",
    "slug": "ConvoWave",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/images/Image_fx.jpg",
    "scheme": "boltapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "We need camera access to allow you to upload a profile picture.",
        "NSPhotoLibraryUsageDescription": "We need photo library access to let you select a profile picture.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.emmanard9.ConvoWave"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/images/Image_fx.jpg",
        "backgroundColor": "#ffffff",
        
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.emmanard9.ConvoWave"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9f0f3628-cfae-4560-87ab-23728c282a9c"
      }
    },
    "owner": "emmanard9"
  }
}
